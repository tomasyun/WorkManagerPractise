<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yt.workmanagerpractise">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yt.workmanagerpractise.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yt.workmanagerpractise.inputoutput.InputOutputWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.periodic.PeriodicWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.constraints.ConstraintsWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.cancel.CancelWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.then.OrderWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.combine.CombineWorkerActivity" />
        <activity android:name="com.yt.workmanagerpractise.taskchainstream.TaskCharinStreamActivity" />
        <activity android:name="com.yt.workmanagerpractise.unique.UniqueWorkerActivity" />
    </application>

</manifest>